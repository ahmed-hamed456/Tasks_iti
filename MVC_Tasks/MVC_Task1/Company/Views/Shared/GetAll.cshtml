@{
}
<h1>Student Data</h1>
<table>
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Age</th>
            <th>Salary</th>
            <th>Address</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model){
        <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Salary</td>
                <td>@item.Address</td>
                <td>@item.Email</td>
        </tr>
          }
    </tbody>
</table>